  },
  "keywords": [],
  "author": "augusto",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "nodemon": "^3.0.1",
    "prettier": "3.0.3",
    "sequelize-cli": "^6.6.1",
    "sucrase": "^3.34.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cpf": "^2.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.4.3",
